{
  "name": "@bphu/zustand-react",
  "version": "0.1.3",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bp-hu/zustand-react"
  },
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    },
    "./utils": {
      "types": "./dist/utils.d.ts",
      "import": "./dist/utils.js",
      "default": "./dist/utils.js"
    }
  },
  "typesVersions": {
    "*": {
      "utils": [
        "./dist/utils.d.ts"
      ]
    }
  },
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist/**/*",
    "README.md"
  ],
  "scripts": {
    "build": "rslib build",
    "dev": "rslib build --watch",
    "format": "prettier --write .",
    "lint": "eslint ."
  },
  "devDependencies": {
    "@eslint/js": "^9.12.0",
    "@rsbuild/core": "^1.1.13",
    "@rsbuild/plugin-react": "^1.1.0",
    "@rslib/core": "^0.2.2",
    "@storybook/addon-essentials": "^8.4.7",
    "@types/lodash.pick": "^4.4.9",
    "@types/react": "^19.0.2",
    "eslint": "^9.12.0",
    "globals": "^15.11.0",
    "prettier": "^3.3.3",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "storybook": "^8.4.7",
    "storybook-addon-rslib": "^0.1.7",
    "storybook-react-rsbuild": "^0.1.7",
    "typescript": "^4.9.5",
    "typescript-eslint": "^8.8.1",
    "zustand": "^4.5.5"
  },
  "peerDependencies": {
    "react": ">=16.9.0",
    "react-dom": ">=16.9.0",
    "zustand": ">=4.5.5"
  },
  "private": false,
  "dependencies": {
    "lodash.pick": "^4.4.0"
  },
  "keywords": [
    "zustand",
    "react",
    "state",
    "utils",
    "hooks"
  ]
}